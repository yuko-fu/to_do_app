<h1>アカウント編集</h1>

<%= form_with(model: @user, local: true) do |f| %>  
  <%= f.label :名前 %>
  <%= f.text_field :name %>

  <%= f.label :メールアドレス %>
  <%= f.email_field :email %>

  <%= f.label :パスワード %>
  <%= f.password_field :password %>

  <%= f.label :パスワード確認用 %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "編集完了" %>
<% end %>